@model WorkingShift
<div class="modal fade" id="DeleteWorkingShift-@Model.WorkingShiftId" aria-labelledby="DeleteworkingshiftLabal" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title">Delete WorkingShirt</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>

			</div>
			<div class="modal-body">
				<form asp-action="DeleteWorkingShift" method="post">

					<div asp-validation-summary="ModelOnly" class="text-danger"></div>
					<div class="form-group">
						<input type="hidden" asp-for="WorkingShiftId" />
						<p>Are you sure to delete this workingshift?</p>
					</div>

					
					<div class="modal-footer">
						<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
						<button type="submit" class="btn btn-primary">Delete</button>
					</div>


				</form>
			</div>

		</div>

	</div>


</div>